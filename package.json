{
    "name": "unused-file-check",
    "version": "1.0.0",
    "description": "TODO: 未引用文件检测, 删除, 一键配置",
    "main": "dist/unused-file-check.cjs.js",
    "module": "dist/unused-file-check.es.js",
    "directories": {
        "test": "test"
    },
    "scripts": {
        "build": "rollup -c",
        "prettier": "prettier  --write src/*",
        "test": "echo \"Error: no test specified\"",
        "lint": "eslint --fix",
        "commit-msg": "commitlint -e $GIT_PARAMS",
        "commit-name": "node .gitauthor.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/OBKoro1/unused-file-check"
    },
    "keywords": [
        "no-import-file",
        "unless-files-webpack-plugin",
        "deadFile",
        "deadFile-check",
        "unless-file-check",
        "unused-file-remove",
        "未引用文件检测",
        "未引用代码检测"
    ],
    "author": {
        "name": "OBKoro1",
        "email": "obkoro1@foxmail.com"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/OBKoro1/unused-file-check/issues"
    },
    "homepage": "https://github.com/OBKoro1/unused-file-check",
    "engines": {
        "node": ">=10.0.0"
    },
    "files": [
        "dist/[^.]*",
        "types/[^.]*",
        "docs/[^.]*",
        "doc/[^.]*"
    ],
    "branchNameLinter": {
        "skip": [
            "master"
        ]
    },
    "devDependencies": {
        "@commitlint/cli": "11.0.0",
        "@commitlint/config-conventional": "11.0.0",
        "@rollup/plugin-buble": "0.21.3",
        "@rollup/plugin-commonjs": "22.0.0",
        "@rollup/plugin-json": "4.1.0",
        "all-contributors-cli": "6.17.2",
        "eslint": "8.17.0",
        "eslint-config-standard": "17.0.0",
        "husky": "^4.2.5",
        "package-tls": "1.2.2",
        "path": "0.12.7",
        "prettier": "2.1.1",
        "rollup": "^2.26.11",
        "rollup-plugin-delete": "2.0.0",
        "rollup-plugin-terser": "^7.0.2"
    },
    "dependencies": {
        "shelljs": "0.8.5"
    }
}
